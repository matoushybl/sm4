%% Zpracovani volby typu prace
\DeclareOption{semestral}%sazba semestralni prace
  {%
    \ifdefined\akt@type
      \@latex@warning@no@line{Typ prace zmenen na semestralni/Document type changed to semestral}%
    \fi
    \global\let\akt@type=\type@semestral
  }%
\DeclareOption{bachelor}%sazba bakalarske prace
  {%
    \ifdefined\akt@type
      \@latex@warning@no@line{Typ prace zmenen na bakalarskou/Document type changed to bachelor}%
    \fi
    \global\let\akt@type=\type@bachelor
  }%
\DeclareOption{master}%sazba diplomove prace
  {%
    \ifdefined\akt@type
      \@latex@warning@no@line{Typ prace zmenen na diplomovou/Document type changed to master}%
    \fi
    \global\let\akt@type=\type@diploma
  }%
\DeclareOption{treatise}%sazba pojednani o doktorske praci
  {%
    \ifdefined\akt@type
      \@latex@warning@no@line{Typ prace zmenen na pojednani/Document type changed to treatise}%
    \fi
    \global\let\akt@type=\type@treatise
  }%
\DeclareOption{doctoral}%sazba doktorske prace
  {%
    \ifdefined\akt@type
      \@latex@warning@no@line{Typ prace zmenen na disertacni/Document type changed to doctoral}%
    \fi
    \global\let\akt@type=\type@phd
  }%

